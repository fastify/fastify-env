    "env-schema": "^6.0.0",
    "fastify-plugin": "^5.0.0"
  },
  "devDependencies": {
    "@fastify/pre-commit": "^2.1.0",
    "@types/node": "^22.0.0",
    "c8": "^10.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "fastify": "^5.0.0",
    "neostandard": "^0.12.0",
    "tsd": "^0.31.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
